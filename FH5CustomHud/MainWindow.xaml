<Window x:Class="FH5CustomHud.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FH5CustomHud"
        mc:Ignorable="d"
        Title="FH5 Custom Hud" Height="200" Width="500"
        Closing="SettingsWindow_OnClosing">
    <Grid HorizontalAlignment="Center" Margin="20">
        <CheckBox Content="Auto Hide" Checked="ToggleButton_OnChecked" Unchecked="ToggleButton_OnUnchecked"></CheckBox>
    </Grid>
</Window>
